{
    "name": "bperel/dm-server",
    "license": "Apache-2.0",
    "description": "DucksManager server REST API endpoint",
    "type": "library",
    "keywords": ["ducksmanager", "whattheduck"],
    "homepage": "https://ducksmanager.net",
    "authors": [
        {
            "name": "Bruno Perel",
            "homepage": "https://github.com/bperel"
        }
    ],
    "support": {
        "issues": "https://github.com/bperel/dm-server/issues"
    },
    "require": {
        "php": "^7.2.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.2",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/flex": "^1.2",
        "symfony/framework-bundle": "4.2.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer": "4.2.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "4.2.*",
        "symfony/css-selector": "4.2.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "4.2.*",
        "symfony/web-server-bundle": "4.2.*",
	    "ext-exif": "*"
	}
}
